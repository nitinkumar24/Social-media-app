
<% if @is_liked %>
$('#like_<%= @post.id%>').addClass("likelink faheart");
$('#count_like_<%= @post.id%>').html(<%=@post.likes.count%>);
// noty({text: 'Tweet was Liked', type: 'alert'});
<% else %>
$('#like_<%= @post.id%>').html("<i class='fa fa-heart'></i>".html_safe);
$('#count_like_<%= @post.id%>').html(<%=@post.likes.count%>);
//noty({text: 'Tweet was UnLiked', type: 'alert'});

<% end %>


<% if @is_disliked %>

$('#dislike_<%= @post.id%>').html("UnDisLike");
$('#count_dislike_<%= @post.id%>').html(<%=@post.dislikes.count%>);

// noty({text: 'Tweet was Liked', type: 'alert'});

<% else %>

$('#dislike_<%= @post.id%>').html("DisLike");
$('#count_dislike_<%= @post.id%>').html(<%=@post.dislikes.count%>);
//noty({text: 'Tweet was UnLiked', type: 'alert'});

<% end %>