<body>
<script>
    $('.ui.dropdown')
        .dropdown();

</script>
<% if notice   %>
    <script type="text/javascript">
        noty({text: '<%= notice %>',
            type: 'alert',
            theme: 'metroui',
            layout: 'topCenter',
            timeout: 2000,
            animation: {
                open : 'animated fadeInLeft',
                close: 'animated fadeOutRight'
            }
        });
    </script>
<% end %>


<% if alert %>
    <script type="text/javascript">
        noty({
            text: '<%= alert %>',
            type: 'alert',
            theme: 'metroui',
            layout: 'topCenter',
            timeout: 2000,
            animation: {
                open : 'animated fadeInLeft',
                close: 'animated fadeOutRight'
            }
        });
    </script>
<% end %>



