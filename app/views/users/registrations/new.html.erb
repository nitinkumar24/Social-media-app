<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>



  <div class="field">
   
    <%= f.text_field :name, autofocus: true,placeholder: "Fullname" %>
  </div>

  <div class="field">
 
    <%= f.email_field :email, autofocus: true ,placeholder: "email"%>
  </div>
    <%= f.label :College %>
    <select name="college" id="college_id">
        <option value="Krishna institute of engineering and technology,Ghaziabad">Krishna institute of engineering and technology,Ghaziabad</option>
        
</select><br />
 <%= f.label :Birthday %>
  <%= date_select :recipient, :birthday, 
               {
                :order => [:day, :month, :year], 
                :start_year => 1980, 
                :end_year => 2005
               }, 
               {:class => "input-mini"}  %>
<br>
<input type="radio"  value="male" name="gender"> Male
  <input type="radio" value="female" name="gender"> Female<br>


  <div class="field">
    
   
    <%= f.password_field :password, autocomplete: "off" ,placeholder: "password"%>
  </div>
 <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
  <div class="field">
    
    <%= f.password_field :password_confirmation, autocomplete: "off",placeholder: "confirm-password" %>
  </div>

  <div class="actions">
    <%= f.submit "Create Account" %>
  </div>
<% end %>

<%= render "users/shared/links" %>
