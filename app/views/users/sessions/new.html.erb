
<head>
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<style>

   body{
      margin:0px;
      padding:0px;
      font-family: 'Source Sans Pro', sans-serif;
      background: url("http://localhost:3000/system/posts/avatars/000/000/021/medium/final1.png?1518359191");
   }


</style>

<div></div>
<div id="signin-page" class="animated ">
   <div class="signinPageLeft">
      <h3 class=''>Socio_Grams</h3>
   </div>

   <div id="sign-in-form-section" >
      <div class="sign-in-form "  >
         <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

             <p class="form-topic"><i class="fa fa-sign-in" aria-hidden="true"></i>
                Sign-In</p><hr style="color: #721615"><br>
             <div class="google_link">
                <%- if devise_mapping.omniauthable? %>
                    <%- resource_class.omniauth_providers.each do |provider| %>

                        <img src="data:image/png;base64,">

                        <input type="button" value="Sign in with google" onclick="window.location.href='/users/auth/google_oauth2'" />

                    <% end -%>
                <% end -%>
             </div>
             <p style="text-align:center;margin-bottom:5px;color: white">OR</p>


             <%= f.email_field :email, autofocus: true ,placeholder:"Email",required: true,class:"user_email_field"%>
             <%= f.password_field :password, autocomplete: "off",placeholder:"Password",required: true,class:"user_password_field" %>

             <br>
             <% if devise_mapping.rememberable? -%>

                 <%= f.check_box :remember_me %>
                 <%= f.label :remember_me,class:"remember_me" %>

             <% end -%>
             <br>

             <%= f.submit "Log in" ,class:"login-button"%><br>

             <hr>
             <div style="padding-top: 10px;">
                <%= render "users/shared/links" %>
             </div>


         <% end %>


      </div>
   </div>

</div>
<div class="typed-slogan">
   <h3 id="typed-sample" style="color: #730E15"></h3>
</div>
